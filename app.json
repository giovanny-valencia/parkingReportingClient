{
  "expo": {
    "name": "parkingClient",
    "slug": "parkingClient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "scheme": "parkingClient",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.parkingClient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.parkingClient",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDY14rH4Rt7ndC5H_YdPsCCKZB_8NpLXlI"
        }
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
